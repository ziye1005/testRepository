<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="edu.njust.mapper.oracle.NodeMapper">
    <select id="findIdByNameAndType" resultType="int">
        select "id" from "node" where "name" = #{name} and "type" = #{type}
    </select>

</mapper>